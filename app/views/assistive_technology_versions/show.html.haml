-title = "Showing  #{@assistive_technology_version}"
-meta title: title
-cache ['v1', admin?,  @assistive_technology_version] do
  .entity-outer
    .entity
      .entity-inner
        %header{role: "header"}
          %h1#page-title{tabindex: -1}=title
        %table.entity-table
          %caption.sr-only Key value pairs for this assistive_technology_version
          %tbody
            %tr
              %th{scope: "row"}<
                Assistive technology
              %td<
                = @assistive_technology_version.assistive_technology
            %tr
              %th{scope: "row"}<
                Version
              %td<
                = @assistive_technology_version.version
            %tr
              %th{scope: "row"}<
                Notes
              %td<
                = @assistive_technology_version.notes
        %nav.show-nav{role:"navigation"}
          = link_to assistive_technology_versions_path, class: 'btn btn-info', title: "View the assistive_technology_versions index" do
            %i.fa.fa-fw.fa-list{title: 'List'}
            Assistive_technology_versions
          = link_to edit_assistive_technology_version_path(@assistive_technology_version), title: "Edit this assistive_technology_version", class: 'btn btn-primary' do
            %i.fa.fa-fw.fa-pencil{title: 'Pencil'}
            Edit
